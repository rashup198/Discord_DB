{
    "compilerOptions": {
      "target": "ES2020",
      "lib": ["ES2020"],
      "module": "commonjs",
      "outDir": "./dist",
      "rootDir": ".",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "declaration": true,
      "sourceMap": true,
      "downlevelIteration": true  
    },
    "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "dist", "examples"]
  }